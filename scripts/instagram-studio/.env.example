# Instagram Studio Environment Variables

# Password protection (SHA-256 hash of your password)
VITE_PASSWORD_HASH=your_sha256_hash_here

# Cloudinary sync function URL (on main site)
VITE_SYNC_FUNCTION_URL=https://lemonpost.studio/.netlify/functions/instagram-studio-sync

# Instagram OAuth (Phase 3)
VITE_INSTAGRAM_APP_ID=your_app_id
VITE_INSTAGRAM_REDIRECT_URI=https://studio.lemonpost.studio/auth/callback

# Auth function URL (on main site)
VITE_AUTH_FUNCTION_URL=https://lemonpost.studio/.netlify/functions/instagram-auth
